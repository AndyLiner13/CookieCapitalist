
<Component x:Name="Instrument_Altimeter_Root"
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
  mc:Ignorable="d">
  <Component.DynamicProperties>
    <DynamicProperty Name="Altitude" Type="Float"/>
  </Component.DynamicProperties>
  <Grid x:Name="Altimeter_Master" HorizontalAlignment="Center" VerticalAlignment="Center">
    <Component Source="/Root;component/Components/Instruments_Generic/Instrument_Generic_Housing.xaml" HorizontalAlignment="Center" VerticalAlignment="Center"/>
    <Component Source="/Root;component/Components/Instruments_Generic/Instrument_Generic_Bevel.xaml" HorizontalAlignment="Center" VerticalAlignment="Center"/>
    <Component Source="/Root;component/Components/Instruments_Generic/Instrument_Generic_Socket.xaml"/>
    <Component Source="/Root;component/Components/Instruments_Gauges/Gauge_Altimeter.xaml" HorizontalAlignment="Center" VerticalAlignment="Center" Altitude_Float="{Binding Path=Altitude_Float, ElementName=Instrument_Altimeter_Root}"/>
    <Component Source="/Root;component/Components/Instruments_Generic/Instrument_Generic_Glint.xaml" HorizontalAlignment="Center" VerticalAlignment="Center"/>
    <Component Source="/Root;component/Components/Instruments_Generic/Instrument_Generic_Ring.xaml" HorizontalAlignment="Center" VerticalAlignment="Center"/>
    <Component Source="/Root;component/Components/Instruments_Generic/Instrument_Generic_Labels.xaml" Label_Bottom_Str="| ALT x 100FT |" Label_Top_Str="ALTITUDE"/>
  </Grid>
</Component>