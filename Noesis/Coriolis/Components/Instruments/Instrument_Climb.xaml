
<Component x:Name="Instrument_Climb_Root"
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
  mc:Ignorable="d">
  <Component.DynamicProperties>
    <DynamicProperty Name="Climb" Type="Float"/>
  </Component.DynamicProperties>
  <Grid x:Name="Climb_Master" HorizontalAlignment="Center" VerticalAlignment="Center">
    <Component Source="/Root;component/Components/Instruments_Generic/Instrument_Generic_Housing.xaml" HorizontalAlignment="Center" VerticalAlignment="Center"/>
    <Component Source="/Root;component/Components/Instruments_Generic/Instrument_Generic_Bevel.xaml" HorizontalAlignment="Center" VerticalAlignment="Center"/>
    <Component Source="/Root;component/Components/Instruments_Generic/Instrument_Generic_Socket.xaml"/>
    <Component Source="/Root;component/Components/Instruments_Gauges/Gauge_Climb.xaml" HorizontalAlignment="Center" VerticalAlignment="Center" Climb_Float="{Binding Path=Climb_Float, ElementName=Instrument_Climb_Root}"/>
    <Component Source="/Root;component/Components/Instruments_Generic/Instrument_Generic_Glint.xaml" HorizontalAlignment="Center" VerticalAlignment="Center"/>
    <Component Source="/Root;component/Components/Instruments_Generic/Instrument_Generic_Ring.xaml" HorizontalAlignment="Center" VerticalAlignment="Center"/>
    <Component Source="/Root;component/Components/Instruments_Generic/Instrument_Generic_Labels.xaml" Label_Bottom_Str="| VSPEED x 100FT/MIN |" Label_Top_Str="VSPEED"/>
  </Grid>
</Component>