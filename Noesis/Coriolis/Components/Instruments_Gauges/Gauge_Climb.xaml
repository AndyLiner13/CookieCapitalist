
<Component x:Name="Climb_Root"
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
  mc:Ignorable="d" xmlns:noesis="clr-namespace:NoesisGUIExtensions">
  <Component.Resources>
    <Storyboard x:Key="Gauge_Climb_IdleAnimation" RepeatBehavior="Forever" AutoReverse="True">
      <DoubleAnimationUsingKeyFrames Storyboard.TargetName="Rotating_Needles_IdleAnimation" Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[2].(RotateTransform.Angle)">
        <EasingDoubleKeyFrame KeyTime="00:00:00" Value="-3"/>
        <EasingDoubleKeyFrame KeyTime="00:00:00.5000000" Value="3">
          <EasingDoubleKeyFrame.EasingFunction>
            <BounceEase EasingMode="EaseInOut" Bounciness="3" Bounces="2"/>
          </EasingDoubleKeyFrame.EasingFunction>
        </EasingDoubleKeyFrame>
      </DoubleAnimationUsingKeyFrames>
    </Storyboard>
  </Component.Resources>
  
  <Component.DynamicProperties>
    <DynamicProperty Name="Climb" Type="Float"/>
  </Component.DynamicProperties>

  <Grid x:Name="Gauge_Climb_Master" Width="180" Height="180">
    <Grid x:Name="Display">
      <noesis:RadialTicks x:Name="RadialTicks_Minor" TickWidth="4" TickLength="8" Padding="0" Spacing="5" Maximum="90" MinAngle="162" MaxAngle="-162" RenderTransformOrigin="0.5,0.5">
        <noesis:RadialTicks.RenderTransform>
          <TransformGroup>
            <ScaleTransform/>
            <SkewTransform/>
            <RotateTransform Angle="-90"/>
            <TranslateTransform/>
          </TransformGroup>
        </noesis:RadialTicks.RenderTransform>
        <noesis:RadialTicks.Brush>
          <SolidColorBrush Color="{DynamicResource White1}"/>
        </noesis:RadialTicks.Brush>
      </noesis:RadialTicks>
      <Grid x:Name="Series_5">
        <noesis:RadialTicks x:Name="RadialTicks_5" TickLength="16" TickWidth="4" Padding="0" MinAngle="-126" MaxAngle="-54" Spacing="10" Maximum="10">
          <noesis:RadialTicks.Brush>
            <SolidColorBrush Color="{DynamicResource White1}"/>
          </noesis:RadialTicks.Brush>
        </noesis:RadialTicks>
        <noesis:RadialNumbers x:Name="RadialNumber_5_Top" Spacing="10" Padding="28" FontFamily="OCR A" FontSize="18" Minimum="5" Maximum="10" MaxAngle="180" MinAngle="-50">
          <noesis:RadialNumbers.Brush>
            <SolidColorBrush Color="{DynamicResource White1}"/>
          </noesis:RadialNumbers.Brush>
        </noesis:RadialNumbers>
        <noesis:RadialNumbers x:Name="RadialNumber_5_Bottom" Spacing="10" FontFamily="OCR A" FontSize="18" MinAngle="-125" MaxAngle="-50" Maximum="10" Minimum="5" Padding="28">
          <noesis:RadialNumbers.Brush>
            <SolidColorBrush Color="{DynamicResource White1}"/>
          </noesis:RadialNumbers.Brush>
        </noesis:RadialNumbers>
        <noesis:RadialBorder BorderWidth="4" MinAngle="-25" MaxAngle="-4" RenderTransformOrigin="0.5,0.5">
          <noesis:RadialBorder.RenderTransform>
            <TransformGroup>
              <ScaleTransform/>
              <SkewTransform/>
              <RotateTransform Angle="-90"/>
              <TranslateTransform/>
            </TransformGroup>
          </noesis:RadialBorder.RenderTransform>
          <noesis:RadialBorder.Brush>
            <SolidColorBrush Color="{DynamicResource White1}"/>
          </noesis:RadialBorder.Brush>
        </noesis:RadialBorder>
        <Ellipse Width="16" Height="16" StrokeThickness="4" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="18,0,0,0">
          <Ellipse.Stroke>
            <SolidColorBrush Color="{DynamicResource White1}"/>
          </Ellipse.Stroke>
        </Ellipse>
        <noesis:RadialBorder RenderTransformOrigin="0.5,0.5" BorderWidth="4" MaxAngle="25" MinAngle="4">
          <noesis:RadialBorder.RenderTransform>
            <TransformGroup>
              <ScaleTransform/>
              <SkewTransform/>
              <RotateTransform Angle="-90"/>
              <TranslateTransform/>
            </TransformGroup>
          </noesis:RadialBorder.RenderTransform>
          <noesis:RadialBorder.Brush>
            <SolidColorBrush Color="{DynamicResource White1}"/>
          </noesis:RadialBorder.Brush>
        </noesis:RadialBorder>
      </Grid>
      <Grid x:Name="Series_10">
        <noesis:RadialTicks x:Name="RadialTicks_10" TickWidth="4" Padding="0" TickLength="16" MinAngle="-162" MaxAngle="-18" Maximum="10" Spacing="10">
          <noesis:RadialTicks.Brush>
            <SolidColorBrush Color="{DynamicResource White1}"/>
          </noesis:RadialTicks.Brush>
        </noesis:RadialTicks>
        <noesis:RadialNumbers x:Name="RadialNumber_10_Top" FontFamily="OCR A" MinAngle="-20" MaxAngle="0" FontSize="24" Maximum="20" Spacing="20" Padding="32" Minimum="10">
          <noesis:RadialNumbers.Brush>
            <SolidColorBrush Color="{DynamicResource White1}"/>
          </noesis:RadialNumbers.Brush>
        </noesis:RadialNumbers>
        <noesis:RadialNumbers x:Name="RadialNumber_10_Bottom" Padding="32" Spacing="20" FontFamily="OCR A" FontSize="24" Minimum="10" Maximum="20" MinAngle="-160" MaxAngle="0">
          <noesis:RadialNumbers.Brush>
            <SolidColorBrush Color="{DynamicResource White1}"/>
          </noesis:RadialNumbers.Brush>
        </noesis:RadialNumbers>
      </Grid>
      <Grid x:Name="Series_15">
        <noesis:RadialTicks x:Name="RadialTicks_15" TickWidth="4" TickLength="16" Padding="0" Spacing="10" Maximum="10" MinAngle="36" MaxAngle="144">
          <noesis:RadialTicks.Brush>
            <SolidColorBrush Color="{DynamicResource White1}"/>
          </noesis:RadialTicks.Brush>
        </noesis:RadialTicks>
        <noesis:RadialNumbers x:Name="RadialNumber_15_Top" Padding="32" FontFamily="OCR A" FontSize="18" MinAngle="35" MaxAngle="-180" Maximum="30" Minimum="15" Spacing="30">
          <noesis:RadialNumbers.Brush>
            <SolidColorBrush Color="{DynamicResource White1}"/>
          </noesis:RadialNumbers.Brush>
        </noesis:RadialNumbers>
        <noesis:RadialNumbers x:Name="RadialNumbers_15_Bottom" Padding="32" Spacing="30" FontFamily="OCR A" FontSize="18" Minimum="15" Maximum="30" MinAngle="145" MaxAngle="180">
          <noesis:RadialNumbers.Brush>
            <SolidColorBrush Color="{DynamicResource White1}"/>
          </noesis:RadialNumbers.Brush>
        </noesis:RadialNumbers>
      </Grid>
      <Grid x:Name="Series_20">
        <noesis:RadialTicks x:Name="RadialTicks_20" TickWidth="4" TickLength="16" Padding="0" Spacing="10" MinAngle="72" MaxAngle="108" Maximum="10">
          <noesis:RadialTicks.Brush>
            <SolidColorBrush Color="{DynamicResource White1}"/>
          </noesis:RadialTicks.Brush>
        </noesis:RadialTicks>
        <TextBlock Text="20" TextWrapping="Wrap" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="24" FontFamily="OCR A" Margin="0,0,16,0">
          <TextBlock.Foreground>
            <SolidColorBrush Color="{DynamicResource White1}"/>
          </TextBlock.Foreground>
        </TextBlock>
        <noesis:RadialBorder RenderTransformOrigin="0.5,0.5" Padding="6" MinAngle="-18" MaxAngle="18" BorderWidth="4">
          <noesis:RadialBorder.RenderTransform>
            <TransformGroup>
              <ScaleTransform/>
              <SkewTransform/>
              <RotateTransform Angle="90"/>
              <TranslateTransform/>
            </TransformGroup>
          </noesis:RadialBorder.RenderTransform>
          <noesis:RadialBorder.Brush>
            <SolidColorBrush Color="{DynamicResource White1}"/>
          </noesis:RadialBorder.Brush>
        </noesis:RadialBorder>
      </Grid>
    </Grid>
    <Grid x:Name="Rotating_Needles_IdleAnimation" RenderTransformOrigin="0.5,0.5">
      <Grid.RenderTransform>
        <TransformGroup>
          <ScaleTransform/>
          <SkewTransform/>
          <RotateTransform/>
          <TranslateTransform/>
        </TransformGroup>
      </Grid.RenderTransform>
      <Grid x:Name="Rotating_Needles_Origin" RenderTransformOrigin="0.5,0.5">
        <Grid.RenderTransform>
          <TransformGroup>
            <ScaleTransform/>
            <SkewTransform/>
            <RotateTransform Angle="90"/>
            <TranslateTransform/>
          </TransformGroup>
        </Grid.RenderTransform>
        <Grid x:Name="Rotating_Needles" RenderTransformOrigin="0.5,0.5">
          <Grid.RenderTransform>
            <TransformGroup>
              <ScaleTransform/>
              <SkewTransform/>
              <RotateTransform/>
              <TranslateTransform/>
            </TransformGroup>
          </Grid.RenderTransform>
          <Grid x:Name="NeedleFlipper" RenderTransformOrigin="0.5,0.5">
            <Grid.RenderTransform>
              <TransformGroup>
                <ScaleTransform/>
                <SkewTransform/>
                <RotateTransform Angle="180"/>
                <TranslateTransform/>
              </TransformGroup>
            </Grid.RenderTransform>
            <noesis:RadialNeedle MinAngle="-160" MaxAngle="160" Maximum="20" Minimum="-20" Value="{Binding Path=Climb_Float, ElementName=Climb_Root}">
              <Component Source="/Root;component/Components/Generic/Needle_Medium.xaml"/>
            </noesis:RadialNeedle>
          </Grid>
          <noesis:RadialNeedle MinAngle="-160" MaxAngle="160" Maximum="20" Value="{Binding Path=Climb_Float, ElementName=Climb_Root}" Minimum="-20">
            <Component Source="/Root;component/Components/Generic/Needle_MediumCounterweight.xaml"/>
          </noesis:RadialNeedle>
        </Grid>
      </Grid>
    </Grid>
    <Component Source="/Root;component/Components/Instruments_Generic/Instrument_Generic_Fastener.xaml" Width="8" Height="8"/>

    <Grid.Triggers>
      <EventTrigger RoutedEvent="Loaded">
        <BeginStoryboard x:Name="Gauge_Climb_IdleAnimation_Begin" Storyboard="{StaticResource Gauge_Climb_IdleAnimation}"/>
      </EventTrigger>
    </Grid.Triggers>
  </Grid>

</Component>