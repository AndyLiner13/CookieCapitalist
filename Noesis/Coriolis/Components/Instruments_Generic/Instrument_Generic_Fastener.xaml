
<Component
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
  mc:Ignorable="d">
  <Grid x:Name="Instrument_Fastener_Master">
    <Grid.Effect>
      <DropShadowEffect ShadowDepth="4" Direction="-90" BlurRadius="4" Color="{DynamicResource Grey1}"/>
    </Grid.Effect>
    <Viewbox>
      <Grid x:Name="Parts" Width="16" Height="16">
        <Ellipse x:Name="Base" Stroke="#FF000000" Fill="#FF191919"/>
        <Ellipse x:Name="Dark1" RenderTransformOrigin="0.5,0.5">
          <Ellipse.RenderTransform>
            <TransformGroup>
              <ScaleTransform/>
              <SkewTransform/>
              <RotateTransform Angle="-135"/>
              <TranslateTransform/>
            </TransformGroup>
          </Ellipse.RenderTransform>
          <Ellipse.Effect>
            <BlurEffect Radius="1"/>
          </Ellipse.Effect>
          <Ellipse.Fill>
            <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
              <LinearGradientBrush.RelativeTransform>
                <TransformGroup>
                  <ScaleTransform/>
                  <SkewTransform/>
                  <RotateTransform/>
                  <TranslateTransform/>
                </TransformGroup>
              </LinearGradientBrush.RelativeTransform>
              <LinearGradientBrush.Transform>
                <TransformGroup>
                  <ScaleTransform/>
                  <SkewTransform/>
                  <RotateTransform/>
                  <TranslateTransform/>
                </TransformGroup>
              </LinearGradientBrush.Transform>
              <GradientStop Offset="1"/>
              <GradientStop Color="#00000000" Offset="0.5"/>
              <GradientStop Color="#FF000000" Offset="0.49637324"/>
            </LinearGradientBrush>
          </Ellipse.Fill>
        </Ellipse>
        <Ellipse x:Name="Dark2" RenderTransformOrigin="0.5,0.5">
          <Ellipse.Effect>
            <BlurEffect Radius="1"/>
          </Ellipse.Effect>
          <Ellipse.RenderTransform>
            <TransformGroup>
              <ScaleTransform/>
              <SkewTransform/>
              <RotateTransform Angle="135"/>
              <TranslateTransform/>
            </TransformGroup>
          </Ellipse.RenderTransform>
          <Ellipse.Fill>
            <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
              <LinearGradientBrush.RelativeTransform>
                <TransformGroup>
                  <ScaleTransform/>
                  <SkewTransform/>
                  <RotateTransform/>
                  <TranslateTransform/>
                </TransformGroup>
              </LinearGradientBrush.RelativeTransform>
              <LinearGradientBrush.Transform>
                <TransformGroup>
                  <ScaleTransform/>
                  <SkewTransform/>
                  <RotateTransform/>
                  <TranslateTransform/>
                </TransformGroup>
              </LinearGradientBrush.Transform>
              <GradientStop Offset="1"/>
              <GradientStop Color="#00000000" Offset="0.5"/>
              <GradientStop Color="#FF000000" Offset="0.49637324"/>
            </LinearGradientBrush>
          </Ellipse.Fill>
        </Ellipse>
        <Ellipse x:Name="Rim" StrokeThickness="0.2" HorizontalAlignment="Center" VerticalAlignment="Center" Width="16.1" Height="16.1" Stroke="#FF808080" Margin="-8.05"/>
        <Border x:Name="Divet1" Height="2" Margin="4,0,4,0" RenderTransformOrigin="0.5,0.5" BorderThickness="0.5,0.5,0.5,0.5" Background="#FF050505">
          <Border.BorderBrush>
            <SolidColorBrush Color="#FF000000"/>
          </Border.BorderBrush>
          <Border.RenderTransform>
            <TransformGroup>
              <ScaleTransform/>
              <SkewTransform/>
              <RotateTransform Angle="45"/>
              <TranslateTransform/>
            </TransformGroup>
          </Border.RenderTransform>
        </Border>
        <Border x:Name="Divet2" Height="2" Margin="4,0,4,0" RenderTransformOrigin="0.5,0.5" BorderThickness="0.5,0.5,0.5,0.5" Background="#FF050505">
          <Border.BorderBrush>
            <SolidColorBrush Color="#FF000000"/>
          </Border.BorderBrush>
          <Border.RenderTransform>
            <TransformGroup>
              <ScaleTransform/>
              <SkewTransform/>
              <RotateTransform Angle="-45"/>
              <TranslateTransform/>
            </TransformGroup>
          </Border.RenderTransform>
        </Border>
      </Grid>
    </Viewbox>
  </Grid>
</Component>