
<Component
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
  mc:Ignorable="d" xmlns:noesis="clr-namespace:NoesisGUIExtensions">
  
  <Grid x:Name="LevelHolder_Master" Width="180" Height="180">
    <Grid x:Name="Bracket" ClipToBounds="True">
      <Grid.Clip>
        <RectangleGeometry Rect="0,128,180,180"/>
      </Grid.Clip>
      <Ellipse x:Name="Bracket_Body" StrokeThickness="0">
        <Ellipse.Fill>
          <SolidColorBrush Color="{DynamicResource Grey1}"/>
        </Ellipse.Fill>
      </Ellipse>
      <Ellipse x:Name="Bracket_Body_Depth" StrokeThickness="4">
        <Ellipse.Stroke>
          <SolidColorBrush Color="#FF000000" Opacity="0.25"/>
        </Ellipse.Stroke>
      </Ellipse>
      <Component Source="/Root;component/Components/Instruments_Generic/Instrument_Generic_Fastener.xaml" Width="8" Height="8" VerticalAlignment="Bottom" HorizontalAlignment="Right" Margin="0,0,24,40"/>
      <Component Source="/Root;component/Components/Instruments_Generic/Instrument_Generic_Fastener.xaml" Width="8" Height="8" VerticalAlignment="Bottom" HorizontalAlignment="Left" Margin="24,0,0,40"/>
    </Grid>

    <Component Source="/Root;component/Components/Instruments_Generic/Instrument_Generic_BubbleLevel.xaml" VerticalAlignment="Bottom" Margin="0,0,0,36" HorizontalAlignment="Center"/>
    <Component Source="/Root;component/Components/Instruments_Generic/Instrument_Generic_RotatorKnob.xaml" Width="32" Height="32" Margin="0,0,0,10" VerticalAlignment="Bottom"/>
  
  </Grid>
</Component>