    <GradientStop Color="#FFFFFFFF" Offset="0.33"/>
    <GradientStop Color="#FFFFFFFF" Offset="0.57"/>
    <GradientStop Offset="0.666"/>
    <GradientStop Color="#FFFFFFFF" Offset="0.67"/>
